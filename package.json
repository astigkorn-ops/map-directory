{
  "name": "pano-app",
  "version": "1.0.0",
  "private": true,
  "description": "Interactive panorama viewer with Neon Postgres backend",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently \"npm run server\" \"vite\"",
    "lint": "eslint .",
    "migrate": "node server/migrate.js",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:dev": "node --watch server/index.js",
    "start": "npm run build && npm run server",
    "typecheck": "tsc --noEmit -p tsconfig.app.json"
  },
  "dependencies": {
    "@supabase/supabase-js": "latest",
    "@types/leaflet": "^1.9.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "latest",
    "express": "latest",
    "fs": "latest",
    "jiti": "latest",
    "jsr:": "supabase",
    "leaflet": "latest",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-omnivore": "^0.3.0",
    "less": "latest",
    "lightningcss": "latest",
    "lucide-react": "^0.454.0",
    "next": "16.0.6",
    "pannellum": "latest",
    "path": "latest",
    "pg": "latest",
    "pg-native": "latest",
    "react": "^18.3.1",
    "react-dom": "latest",
    "sass": "latest",
    "sass-embedded": "latest",
    "stylus": "latest",
    "sugarss": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "terser": "latest",
    "tsx": "latest",
    "url": "latest",
    "yaml": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.6"
